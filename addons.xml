<?xml version="1.0" encoding="UTF-8"?>
<addons>
<addon id="plugin.program.kobra.notifications" version="1.0.3"
       name="Notifications" provider-name="tvaddons.ag">
  <requires>
  	<import addon="xbmc.python" version="2.1.0"/>
  	<import addon="script.module.addon.common" optional="true" />
  </requires>
  <extension point="xbmc.python.script" library="default.py">
    <provides>executable</provides>
  </extension>
  <extension point="xbmc.service" library="service.py" start="startup">
  </extension>
  <extension point="xbmc.addon.metadata">
  	<summary>KOBRA NEWS AND UPDATES</summary>
  	<description>Receive Important Kobra news and update notifications.</description>
  	<disclaimer>We take no responsibility for any problems this plugin might in any way, form, shape or size cause.</disclaimer>
  	<language>en</language>
  	<platform>all</platform>
  	<forum>http://forums.tvaddons.ag/forum.php</forum>
  	<website>http://www.tvaddons.ag/</website>
  	<source></source>
  	<email></email>
  </extension>
</addon>

<addon id="plugin.program.wizards.sleeve" name="THE WIZARDS SLEEVE" version="1.0.1" provider-name="KOBRA">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
	<import addon="script.module.addon.common" />
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
        <provides>video executable</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary lang="en">THE WIZARDS SLEEVE</summary>
    <description lang="en">Welcome to The Wizards Sleeve. Here you will find some of the most popular wizards all in one place.</description>
	<website>https://www.facebook.com/groups/KodiCustomBuilds/</website>
    <platform>all</platform>
  </extension>
</addon>

<addon id="plugin.video.kobra.tube"
	name="Kobra Tube"
	version="1.0.0"
	provider-name="Kobra">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
    <import addon="script.module.addon.common" version="2.0.0"/>
    <import addon="plugin.video.youtube" version="5.0.0"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
    <provides>video</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary lang="en">Kobra Tube</summary>
    <description lang="en">The Official YouTube Channel For Kobra</description>
    <platform>all</platform>
    <forum>https://www.facebook.com/groups/KodiCustomBuilds/</forum>
    <website></website>
  </extension>
</addon>

<addon id="plugin.video.kobrawizard" name="Kobra Wizard" version="5.3" provider-name="Kobra Builds">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
	<import addon="script.module.addon.common" />
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
        <provides>video executable</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary lang="en">Kobra Wizard</summary>
    <description lang="en">Kobra Wizard provides easy access to Kobra Builds at the click of a button. No addons are distributed with Kobra Builds. You will be asked if you would like to download each addons as you click on main menu and sub menu items.
Thanks to all Devs who have contributed to this project!</description>
    <platform>all</platform>
  </extension>
</addon>

<addon id="program.kobra.app.installer" name="Kobra App Installer" version="1.0.4" provider-name="Kobra">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
	<import addon="script.module.addon.common" />
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
        <provides>video executable</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary lang="en">Kobra App Instaler.</summary>
    <description lang="en">Kobra App Installer. Browse through the most popular Android apps on the market and install them with ease. The author of this addon takes no responsibilty for any content viewed via these applications or any damage caused by use of of these applications. </description>
	<website></website>
    <platform>all</platform>
  </extension>
</addon>

<addon id="repository.kobra" name="Kobra Repo" version="1.0.1" provider-name="KCB">
	<extension point="xbmc.addon.repository" name="Kobra Repo">
		<info compressed="false">https://raw.github.com/Barroni1977/kobra/master/addons.xml</info>
		<checksum>https://raw.github.com/Barroni1977/kobra/master/addons.xml.md5</checksum>
		<datadir zip="true">https://raw.github.com/Barroni1977/kobra/master/</datadir>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary>Kobra Repository</summary>
		<description>Home of the Kobra wizard</description>
		<platform>all</platform>
	</extension>
</addon>
</addons>
